<!DOCTYPE html>
<html>
  <head>
    <title>PasswordReset</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag "application" %>

  </head>

  <body>
    <% if logged_in? %>
      <%= link_to "Log out", logout_path, method: "delete" %>
    <% else %>
      <%= link_to "Log in", login_path %>
    <% end %>

    <%= yield %>
  </body>
</html>
